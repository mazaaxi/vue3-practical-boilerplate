{
  "name": "vue3-practical-boilerplate",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:api": "npm run db:setup && node server",
    "db:setup": "source ./package.scripts.sh && SetupDB",
    "db:revert": "source ./package.scripts.sh && RevertDB",
    "build:prod": "vue-cli-service build --mode prod",
    "build:dev": "vue-cli-service build --mode dev",
    "build:preview": "vue-cli-service build --mode preview",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint && stylelint ./src/**/*.{vue,scss}",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{vue,js,ts,jsx,tsx}": [
      "vue-cli-service lint"
    ],
    "./src/**/*.{vue,scss}": [
      "stylelint"
    ]
  },
  "dependencies": {
    "@quasar/extras": "^1.15.5",
    "axios": "^0.21.4",
    "core-js": "^3.25.5",
    "dayjs": "^1.10.7",
    "js-common-lib": "https://github.com/mazaaxi/js-common-lib.git",
    "lodash": "^4.17.21",
    "nanoevents": "~5.1.13",
    "nanoid": "^3.1.30",
    "path-to-regexp": "^6.2.0",
    "quasar": "^2.10.0",
    "rfdc": "^1.3.0",
    "ua-parser-js": "^1.0.1",
    "urijs": "^1.19.7",
    "vue": "^3.2.41",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.175",
    "@types/ua-parser-js": "^0.7.36",
    "@types/urijs": "^1.19.17",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
    "@vue/cli-plugin-pwa": "~4.5.13",
    "@vue/cli-plugin-router": "~4.5.13",
    "@vue/cli-plugin-typescript": "~4.5.13",
    "@vue/cli-plugin-unit-jest": "~4.5.13",
    "@vue/cli-service": "~4.5.13",
    "@vue/compiler-sfc": "^3.2.20",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.1.0",
    "commander": "^9.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.19.1",
    "fs-extra": "^10.1.0",
    "husky": "^7.0.4",
    "json-server": "^0.17.0",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.16",
    "postcss-html": "^1.5.0",
    "prettier": "^2.4.1",
    "sass": "1.32.13",
    "sass-loader": "^10.2.0",
    "simple-git": "^3.7.1",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-prettier": "^2.0.0",
    "testdouble": "^3.16.2",
    "testdouble-jest": "^2.0.0",
    "typescript": "~4.8.4",
    "vue-cli-plugin-quasar": "^5.0.1",
    "vue-jest": "^5.0.0-0"
  }
}
