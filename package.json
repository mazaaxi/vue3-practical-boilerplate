{
  "name": "vue3-practical-boilerplate",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "16"
  },
  "scripts": {
    "dev": "vite",
    "api": "npm run db:setup && node server/index.mjs",
    "build": "run-p type-check \"build-only --mode {1}\" --",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "npm run lint:eslint -- --fix && npm run lint:stylelint -- --fix",
    "lint:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:stylelint": "stylelint \"./src/**/*.{vue,scss,sass}\"",
    "db:setup": "source ./package.scripts.sh && SetupDB",
    "db:revert": "source ./package.scripts.sh && RevertDB",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": [
      "npm run lint:eslint"
    ],
    "./**/*.{vue,scss,sass}": [
      "npm run lint:stylelint"
    ]
  },
  "dependencies": {
    "@quasar/extras": "^1.15.5",
    "axios": "^1.1.3",
    "dayjs": "^1.11.6",
    "js-common-lib": "https://github.com/mazaaxi/js-common-lib.git",
    "lodash": "^4.17.21",
    "nanoevents": "^7.0.1",
    "nanoid": "^4.0.0",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^6.2.1",
    "quasar": "^2.10.0",
    "rfdc": "^1.3.0",
    "urijs": "^1.19.11",
    "url": "^0.11.0",
    "vue": "^3.2.38",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.2.3",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/lodash": "^4.14.186",
    "@types/node": "^16.11.56",
    "@types/urijs": "^1.19.19",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "commander": "^9.4.1",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "json-server": "^0.17.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "sass": "1.32.12",
    "simple-git": "^3.14.1",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "~4.7.4",
    "vite": "^3.0.9",
    "vitest": "^0.23.0",
    "vue-tsc": "^0.40.7"
  }
}
